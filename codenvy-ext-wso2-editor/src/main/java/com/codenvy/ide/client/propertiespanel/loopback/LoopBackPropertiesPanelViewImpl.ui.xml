<ui:UiBinder 
             xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field='res' type='com.codenvy.ide.client.EditorResources'/>
    <g:ScrollPanel addStyleNames="{res.editorCSS.fullSize}">
        <g:DockLayoutPanel>
            <g:north size="60.00">
                <g:FlowPanel>
                    <g:Label text="Description"/>
                    <g:TextBox ui:field="description" width="200px"/>
                </g:FlowPanel>
            </g:north>
        </g:DockLayoutPanel>
    </g:ScrollPanel>
</ui:UiBinder>