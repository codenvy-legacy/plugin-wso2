<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://www.exoplaform.org/xml/ns/kernel_1_0.xsd http://www.exoplaform.org/xml/ns/kernel_1_0.xsd"
               xmlns="http://www.exoplaform.org/xml/ns/kernel_1_0.xsd">

    <component>
        <type>com.codenvy.ide.everrest.EverrestInitializer</type>
    </component>
    <component>
        <type>org.everrest.core.impl.ResourceBinderImpl</type>
    </component>
    <component>
        <type>org.everrest.exoplatform.ProvidersRegistry</type>
    </component>

    <component>
        <type>org.everrest.exoplatform.ExoRequestDispatcher</type>
    </component>

    <component>
        <key>RestfulContainerProvider</key>
        <type>org.everrest.exoplatform.container.RestfulContainerProvider</type>
    </component>

    <component>
        <key>everrest</key>
        <type>org.everrest.exoplatform.container.RestfulContainer</type>
    </component>

    <component>
        <key>org.everrest.core.RequestHandler</key>
        <type>org.everrest.exoplatform.ExoRequestHandler</type>
        <init-params>
            <value-param>
                <name>org.everrest.http.method.override</name>
                <value>true</value>
            </value-param>
        </init-params>
    </component>
    <component>
        <type>org.everrest.exoplatform.StartableApplication</type>
    </component>
    <component>
        <type>org.everrest.exoplatform.ExoDependencySupplier</type>
    </component>

    <component>
        <type>org.everrest.services.RestServicesList</type>
    </component>

    <remove-configuration>org.exoplatform.services.rest.impl.ApplicationRegistry</remove-configuration>
    <remove-configuration>org.exoplatform.services.rest.impl.StartableApplication</remove-configuration>
    <remove-configuration>org.exoplatform.services.rest.impl.ProvidersRegistry</remove-configuration>
    <remove-configuration>org.exoplatform.services.rest.impl.RequestHandlerImpl</remove-configuration>
    <remove-configuration>org.exoplatform.services.rest.impl.RequestDispatcher</remove-configuration>
    <remove-configuration>org.exoplatform.services.rest.impl.ResourceBinder</remove-configuration>
    <remove-configuration>org.exoplatform.services.rest.impl.provider.JAXBContextResolver</remove-configuration>

</configuration>