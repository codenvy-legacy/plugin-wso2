<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gmmf="urn:import:org.genmymodel.gmmf.ui"
	xmlns:gmmf.tools="urn:import:org.genmymodel.gmmf.ui.tools"
	xmlns:gmmf.properties="urn:import:org.genmymodel.gmmf.propertypanel">

	<ui:with type="com.codenvy.ide.ext.wso2.client.WSO2Resources.WSO2Style" field="style"/>

	<g:HTMLPanel>
		<g:SplitLayoutPanel ui:field="splitLayout" height="100%" width="100%">
			<g:center>
				<g:FlowPanel styleName="{style.graphicContainer}">
					<gmmf.tools:Toolbar ui:field="toolbar" styleName="{style.toolbar}"  />
					<gmmf:ModelWidget ui:field="modelWidget" styleName="{style.modelWidget}" />
				</g:FlowPanel>
			</g:center>
			<g:south size="325">
				<g:FlowPanel addStyleNames="property-panel" styleName="">
					<g:Label styleName="{style.wso2PropertyPanelTitle}">Properties</g:Label>
					<gmmf.properties:PropertyPanel ui:field="propertyPanel" styleName="{style.wso2PropertyPanel}" />
				</g:FlowPanel>
			</g:south>
		</g:SplitLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder>

