<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field='locale' type='com.codenvy.ide.ext.wso2.client.LocalizationConstant'/>

    <ui:style>
        .spacing {
            margin-bottom: 10px;
        }
    </ui:style>

    <g:DockLayoutPanel unit="PX" width="100%" height="100%">

        <g:north size="26">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{style.spacing}">
                <g:west size="120.0">
                    <g:Label text="{locale.groupID}"/>
                </g:west>
                <g:center>
                    <g:TextBox width="400px" ui:field="groupID"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>

        <g:north size="26">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{style.spacing}">
                <g:west size="120.0">
                    <g:Label text="{locale.artifactId}"/>
                </g:west>
                <g:center>
                    <g:TextBox width="400px" ui:field="artifactID"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>

        <g:north size="26">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{style.spacing}">
                <g:west size="120.0">
                    <g:Label text="{locale.version}"/>
                </g:west>
                <g:center>
                    <g:TextBox width="400px" ui:field="version"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>

        <g:north size="26">
            <g:CheckBox text="{locale.specifyParentPom}" ui:field="parentPomConf"/>
        </g:north>

        <g:north size="26">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{style.spacing}">
                <g:west size="120.0">
                    <g:Label text="{locale.parentGroupId}"/>
                </g:west>
                <g:center>
                    <g:TextBox width="400px" ui:field="parentGroupID"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>

        <g:north size="26">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{style.spacing}">
                <g:west size="120.0">
                    <g:Label text="{locale.parentArtifactId}"/>
                </g:west>
                <g:center>
                    <g:TextBox width="400px" ui:field="parentArtifactID"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>

        <g:north size="26">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{style.spacing}">
                <g:west size="120.0">
                    <g:Label text="{locale.parentVersion}"/>
                </g:west>
                <g:center>
                    <g:TextBox width="400px" ui:field="parentVersion"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>

    </g:DockLayoutPanel>
</ui:UiBinder>