<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:DockLayoutPanel unit="PX" width="100%" height="100%">

        <g:west size="120">
            <g:Label ui:field="resourceNameTitle"/>
        </g:west>

        <g:north size="25">
            <g:TextBox ui:field="resourceName" width="400px"/>
        </g:north>

    </g:DockLayoutPanel>

</ui:UiBinder>